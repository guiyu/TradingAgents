# OpenAI API 配置
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_BASE_URL=https://api.openai.com/v1

# 如果使用自托管或代理API，请修改BASE_URL，例如：
# OPENAI_BASE_URL=https://free.yunwu.ai/v1

# FinnHub API 配置
FINNHUB_API_KEY=your_finnhub_api_key_here

# 飞书机器人配置
FEISHU_WEBHOOK_URL=https://open.feishu.cn/open-apis/bot/v2/hook/your_webhook_id
FEISHU_SECRET=your_feishu_secret_here

# Reddit API 配置 (可选)
REDDIT_CLIENT_ID=your_reddit_client_id
REDDIT_CLIENT_SECRET=your_reddit_client_secret
REDDIT_USER_AGENT=TradingAgents/1.0

# 其他配置
# 设置为false以禁用在线工具（适用于不支持所有API端点的自托管服务器）
ONLINE_TOOLS=true

# 日志级别
LOG_LEVEL=INFO
